pipeline {
    agent any
    stages {
      stage('Init') {
            steps {
                   echo on
                   echo "Hi, this is Harikrishna from LevelUp360"
                   echo "We are Starting the Testing"
             }
      }
        stage('Build') {
             steps {
                   echo on
                   echo "Building Sample Maven Project"
              }
        }
        stage('Deploy') {
             steps {
                   echo on
                   echo "Deploying in Staging Area"
             }
        }
    }
}
